# This defines the root node that Models are added to.
RootModel "ModelRoot" {
  # Use the Faceted shader for models.
  shader_names: ["Faceted"],

  # Start with default uniforms for the Faceted shader.
  blocks: [ USE "T_DefaultFacetedUniforms" ],
}
